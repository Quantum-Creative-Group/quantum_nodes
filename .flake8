[flake8]
max-line-length = 120
exclude =
    .git,
    .vscode,
    __pycache__,
    quantum_nodes/**/__init__.py,
ignore =
    # Imported but unused
    F401,
    # Module level import not at top of file
    E402,
    # Redefinition of unused module
    F811,
    # Syntax error in annotation
    F722,
    # Double quotes found but single quotes preferred
    Q000,